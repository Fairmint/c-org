    "openzeppelin-solidity": "2.3.0",
    "openzeppelin-test-helpers": "0.4.0",
    "papaparse": "5.0.0",
    "tpl-contracts": "1.1.3",
    "truffle": "5.0.26",
    "truffle-hdwallet-provider": "1.0.14",
    "truffle-require": "2.0.15",
    "zos": "2.4.2",
    "zos-lib": "2.4.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb-base": "13.2.0",
